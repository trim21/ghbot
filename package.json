{
  "name": "graphql",
  "version": "0.5.3",
  "description": "",
  "private": true,
  "type": "module",
  "main": "bin/main.ts",
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "format": "prettier --list-different -w ./",
    "format-check": "prettier --check ./",
    "tsc": "tsc --noEmit",
    "lint": "eslint",
    "start": "nodemon ./bin/main.ts",
    "test": "vitest --run",
    "vitest": "vitest",
    "run-file": "node --no-warnings --loader @esbuild-kit/esm-loader",
    "e": "node --no-warnings -r dotenv/config --loader @esbuild-kit/esm-loader",
    "prepare": "husky"
  },
  "dependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@octokit/rest": "^21.0.2",
    "@octokit/webhooks": "^13.3.0",
    "@sinclair/typebox": "^0.33.17",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "ajv-keywords": "^5.1.0",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.8.4",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@typescript-eslint/utils": "^8.16.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.12.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.16.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**.test.ts"
    ],
    "watch": [
      "bin",
      "lib",
      "routes",
      "config.yaml",
      "package.json",
      "tsconfig.json",
      ".env"
    ],
    "delay": 100,
    "execMap": {
      "ts": "node --no-warnings -r dotenv/config --loader @esbuild-kit/esm-loader"
    },
    "env": {
      "NODE_ENV": "development",
      "REF": "development"
    },
    "ext": "ts,js,json,html"
  },
  "author": "",
  "license": "MIT",
  "prettier": {
    "plugins": [
      "prettier-plugin-jsdoc"
    ],
    "tsdoc": true,
    "printWidth": 120,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  }
}
